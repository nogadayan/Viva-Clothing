@model List<Product>
@{
    ViewData["Title"] = "Overzicht";
}

<div class="text-center">
    <h1 class="display-4">Producten</h1>


    @foreach(Product p in Model)
    {
        <div class="product">
            <img src="@p.Voorkant" />
            <p>@p.Name</p>            
        </div>
    }

</div>
